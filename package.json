{
  "name": "ih-mockup-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "maintainers": [
    {
      "name": "Yu Wang",
      "email": "yu.wang6@amd.com"
    }
  ],
  "scripts": {
    "dev": "vite",
    "dev:remote": "vite --host 0.0.0.0 --port 5173",
    "build": "tsc -b && vite build",
    "build:analyze": "tsc -b && vite build --mode analyze",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "validate-model-data": "tsx scripts/validateModelData.ts",
    "validate-markdown": "markdownlint src/modelcards/*.md",
    "check-bundle-size": "tsx scripts/checkBundleSize.ts",
    "generate-model-data": "tsx scripts/generateModelData.ts",
    "convert-blueprints": "tsx scripts/convertToBlueprintCatalog.ts",
    "validate-blueprint-catalog": "tsx scripts/validateBlueprintCatalog.ts",
    "ci:quality": "npm run type-check && npm run lint && npm run format:check",
    "ci:test": "npm run test:coverage",
    "ci:build": "npm run build",
    "ci:security": "npm audit --audit-level=moderate",
    "ci:validate": "npm run validate-model-data && npm run validate-markdown",
    "pre-commit": "npm run ci:quality && npm run ci:test"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@monaco-editor/react": "^4.7.0",
    "axios": "^1.10.0",
    "monaco-editor": "^0.45.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "markdownlint-cli": "^0.38.0",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
